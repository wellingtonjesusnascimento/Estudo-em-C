#include <stdio.h>
#include <stdlib.h>

int main()
{
    //Dentre dos parentêses for, será dada a variável, condição e incremento.
    int i;
    //Respectivamente entre os parentêses do for, apresenta-se a: variável, a condição e o incremento.
    for(i=0; i<=10; i++)
    {
        printf("%d ", i);
    }

    return 0;
}
